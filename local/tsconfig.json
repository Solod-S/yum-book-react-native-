{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "paths": {
      "@/*": ["./*"]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts",
    "nativewind-env.d.ts",
    "app_1/_layout.js",
    "app_1/(tabs)/index.js",
    "app_1/(tabs)/_layout.js",
    "app_1/(tabs)/home.js"
  ]
}
